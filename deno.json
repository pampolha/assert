{
  "name": "assert-bot",
  "version": "1.0.0",
  "description": "Assert bot dependencies",
  "exports": "./src/index.ts",
  "tasks": {
    "start": "deno run --allow-all --env-file src/index.ts",
    "deploy-commands-global": "deno run --allow-all --env-file src/scripts/deployCommands.ts",
    "deploy-commands-debug": "deno run --allow-all --env-file src/scripts/deployCommands.ts --debug",
    "generate-scenario": "deno run --allow-all --env-file src/scripts/generateScenario.ts",
    "dev": "deno run --watch --allow-all --env-file src/index.ts"
  },
  "imports": {
    "@aws-sdk/client-dynamodb": "npm:@aws-sdk/client-dynamodb@^3.848.0",
    "discord.js": "npm:discord.js@^14.21.0",
    "dynamodb-onetable": "npm:dynamodb-onetable@^2.7.7",
    "openai": "npm:openai@^5.20.2",
    "zod": "npm:zod@^4.1.8"
  },
  "license": "CC-BY-NC-ND-4.0",
  "nodeModulesDir": "auto"
}